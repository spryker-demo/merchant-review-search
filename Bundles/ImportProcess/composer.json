{
    "name": "spryker-demo/import-process",
    "type": "library",
    "description": "ImportProcess module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/acl": "^3.13.0",
        "spryker/data-import": "^1.19.1",
        "spryker/event": "^2.9.2",
        "spryker/event-behavior": "^1.24.1",
        "spryker/kernel": "^3.30.0",
        "spryker/user": "^3.14.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
