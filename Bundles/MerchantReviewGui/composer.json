{
    "name": "spryker-demo/merchant-review-gui",
    "type": "library",
    "description": "MerchantReviewGui module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker-demo/merchant-review": "*",
        "spryker/customer": "*",
        "spryker/gui": "*",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "*",
        "spryker/symfony": "*",
        "spryker/util-date-time": "*",
        "spryker/util-sanitize": "*",
        "spryker/util-text": "*"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
