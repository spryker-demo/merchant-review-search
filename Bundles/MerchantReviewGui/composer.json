{
    "name": "spryker-demo/merchant-review-gui",
    "type": "library",
    "description": "MerchantReviewGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-demo/merchant-review": "^1.0.0",
        "spryker/customer": "^7.51.0",
        "spryker/gui": "^3.47.0",
        "spryker/kernel": "^3.70.0",
        "spryker/locale": "^4.1.0",
        "spryker/merchant": "^3.7.0",
        "spryker/symfony": "^3.12.0",
        "spryker/util-date-time": "^1.2.0",
        "spryker/util-sanitize": "^2.3.0",
        "spryker/util-text": "^1.5.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
