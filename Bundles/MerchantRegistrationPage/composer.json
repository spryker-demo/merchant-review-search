{
    "name": "spryker-demo/merchant-registration-page",
    "type": "library",
    "description": "Merchant registrationPage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-demo/merchant-registration": "* as 1.0.0",
        "spryker/application": "^3.30.1",
        "spryker/kernel": "^3.30.0",
        "spryker/store": "^1.16.1",
        "spryker/symfony": "^3.11.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.18",
        "spryker/router": "^1.14.1"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "spryker/router": "ADD SUGGEST DESCRIPTION"
    }
}
