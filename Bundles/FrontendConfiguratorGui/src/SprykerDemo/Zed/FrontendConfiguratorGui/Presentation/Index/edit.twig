{% extends '@Gui/Layout/layout.twig' %}

{% block section_title 'Frontend UI config' | trans %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' with { widget_title: 'Frontend UI config' } %}
        {% block widget_content %}
            {{ form_start(form, {'attr': {'class': 'color-form'}}) }}

            <fieldset class="color-fieldset">
                <legend class="color-legend">Header Colors</legend>

                <div class="color-row">
                    {{ form_widget(form.headerTopbarColorswitch, {'label': false}) }}
                    {{ form_widget(form.headerTopbarColor) }}
                    {{ form_label(form.headerTopbarColor) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.headerTopbarTextColorswitch, {'label': false}) }}
                    {{ form_widget(form.headerTopbarTextColor) }}
                    {{ form_label(form.headerTopbarTextColor) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.headerColorswitch, {'label': false}) }}
                    {{ form_widget(form.headerColor) }}
                    {{ form_label(form.headerColor) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.headerTextColorswitch, {'label': false}) }}
                    {{ form_widget(form.headerTextColor) }}
                    {{ form_label(form.headerTextColor) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.headerNavigationBackgroundswitch, {'label': false}) }}
                    {{ form_widget(form.headerNavigationBackground) }}
                    {{ form_label(form.headerNavigationBackground) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.headerNavigationTextColorswitch, {'label': false}) }}
                    {{ form_widget(form.headerNavigationTextColor) }}
                    {{ form_label(form.headerNavigationTextColor) }}
                </div>
            </fieldset>

            <fieldset class="color-fieldset">
                <legend class="color-legend">Footer Colors</legend>
                <div class="color-row">
                    {{ form_widget(form.footerColorswitch, {'label': false}) }}
                    {{ form_widget(form.footerColor) }}
                    {{ form_label(form.footerColor) }}
                </div>
                <div class="color-row">
                    {{ form_widget(form.footerTextColorswitch, {'label': false}) }}
                    {{ form_widget(form.footerTextColor) }}
                    {{ form_label(form.footerTextColor) }}
                </div>
            </fieldset>

            <fieldset class="color-fieldset">
                <legend class="color-legend">Buttons Colors</legend>
                <div class="color-row">
                    {{ form_widget(form.buttonPrimaryColorswitch, {'label': false}) }}
                    {{ form_widget(form.buttonPrimaryColor) }}
                    {{ form_label(form.buttonPrimaryColor) }}
                </div>
            </fieldset>

            <fieldset class="color-fieldset">
                <legend class="color-legend">Shop Logo Upload</legend>
                <div class="color-row">
                    {{ form_widget(form.logoUrl) }}
                    {{ form_label(form.logoUrl) }}
