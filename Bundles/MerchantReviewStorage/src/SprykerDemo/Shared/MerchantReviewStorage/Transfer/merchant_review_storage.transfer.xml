<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="MerchantReviewStorage">
        <property name="fkMerchant" type="int"/>
        <property name="reviews" type="array" />
    </transfer>

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="data" type="string"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="MerchantReview">
        <property name="idMerchantReview" type="int"/>
        <property name="fkMerchant" type="int"/>
        <property name="customerReference" type="string"/>
        <property name="rating" type="int"/>
        <property name="summary" type="string"/>
        <property name="description" type="string"/>
        <property name="nickname" type="string"/>
    </transfer>
</transfers>
