<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="MerchantReviewStorage">
        <property name="idMerchantReview" type="int"/>
        <property name="averageRating" type="float"/>
        <property name="reviewCount" type="int"/>
    </transfer>

    <transfer name="SynchronizationData">
        <property name="reference" type="string"/>
        <property name="data" type="string"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="MerchantReviewStorageCollection">
        <property name="merchantReviews" type="MerchantReviewStorage[]" singular="merchantReview"/>
        <property name="pagination" type="Pagination"/>
    </transfer>
</transfers>
