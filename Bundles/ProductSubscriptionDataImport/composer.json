{
    "name": "spryker-demo/product-subscription-data-import",
    "type": "library",
    "description": "Product subscription data import module",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "spryker/data-import": "^1.0.0",
        "spryker/kernel": "^3.33.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "config": {
        "sort-packages": true
    }
}
