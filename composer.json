{
    "name": "spryker-demo/merchant-review-search",
    "type": "library",
    "description": "MerchantReviewSearch module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-demo/merchant-review": "^1.0.0",
        "spryker/event-behavior": "^1.25.0",
        "spryker/kernel": "^3.70.0",
        "spryker/publisher-extension": "^1.0.0",
        "spryker/search": "^8.20.0",
        "spryker/search-extension": "^1.2.0",
        "spryker/store": "^1.16.1",
        "spryker/util-encoding": "^2.1.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/elastica": "*",
        "spryker/search-elasticsearch": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "spryker/elastica": "ADD SUGGEST DESCRIPTION",
        "spryker/search-elasticsearch": "ADD SUGGEST DESCRIPTION"
    }
}
